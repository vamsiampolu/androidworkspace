<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
     >
<TextView android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:text="Names of Presidents"/>
<AutoCompleteTextView android:id="@+id/autocompltv"
    android:layout_width="120dp" android:layout_height="wrap_content"/>

<!-- Picker Views -->

<TimePicker
    android:id="@+id/timePicker"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" />

<Button android:id="@+id/btn_timepicker" android:text="Iam all set!" android:layout_width="wrap_content" android:layout_height="wrap_content" android:onClick="onClick"/>"
    </LinearLayout>